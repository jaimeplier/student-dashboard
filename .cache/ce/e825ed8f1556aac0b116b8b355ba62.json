oration:none;\r\n    font-weight:300;\r\n    color:#FFFFFF;\r\n    background-color:$main-color;\r\n    text-align:center;\r\n    transition: all 0.2s;\r\n    \r\n    &:hover{\r\n       background-color:darken($main-color, 15%);\r\n    }\r\n}\r\n\r\n\r\n.icon-user {\r\n  width: 100%;\r\n}\r\n\r\n.user-box {\r\n    font-family: 'CircularStd', serif;\r\n    width: 1104px;\r\n    display: inline-block;\r\n    margin-bottom: 30px;\r\n    /* padding-top: 30px; */\r\n}\r\n\r\nmain {\r\n    padding-top: 30px;\r\n    margin-top: 30px;\r\n}\r\n\r\n.pointer {\r\n    cursor: pointer;\r\n}\r\n \r\n.search-bar {\r\n    padding: 24px 37px 26px 20px;\r\n    display: inline-block;\r\n}\r\n\r\n.border-style {\r\n    display: table-cell;\r\n    width: 150px;\r\n    height: 45px;\r\n    border: solid #E9E9E9 1px;\r\n    border-radius: 2px;\r\n    fill: solid #F9F9F9;\r\n    box-shadow: rgba(0,0,0,0.04);\r\n    display: inline-block;\r\n    vertical-align: middle;\r\n}\r\n\r\n.search {\r\n    width: 250px;\r\n    height: 45px;\r\n    padding-left: 30px;\r\n\r\n    background: url(./assets/search.svg);\r\n    background-repeat: no-repeat;\r\n    background-position: left center;\r\n}\r\n\r\n.search-icon {\r\n   fill: #FA6A00;\r\n   position: center;\r\n   width: 13px;\r\n   height: 13px;\r\n}\r\n\r\n.max-select {\r\n    margin-left: 2rem;\r\n}\r\n\r\n.custom-select {\r\n    font-family: 'CircularStd', serif;\r\n    position: relative;\r\n    background: url(./assets/arrow.svg) no-repeat right;\r\n    background-origin: content-box;\r\n\r\n    padding-left: 2rem;\r\n    height: 100%;\r\n    width: 100%;\r\n    float: left;\r\n  }\r\n  \r\n  .custom-select select {\r\n    display: none; /*hide original SELECT element: */\r\n    -moz-appearance:none; /* Firefox */\r\n    -webkit-appearance:none; /* Safari and Chrome */\r\n    appearance:none;\r\n  }\r\n\r\n  .align-left {\r\n      float: left;\r\n  }\r\n\r\n  .round-img {\r\n      width: 40px;\r\n      height: 40px;\r\n      border-radius: 50%;\r\n  }\r\n\r\n    .card {\r\n        border: solid #E9E9E9 1px;\r\n        margin: 30px;\r\n        padding: 20px;\r\n        padding-bottom: 30px;\r\n        text-align: center;\r\n        width: 260px;\r\n    }\r\n\r\n\r\n    .addIcon {\r\n        width: 20px;\r\n        height: 20px;\r\n        display: inline-block;\r\n        vertical-align: sub;\r\n        background-color: #FFFFFF;\r\n        -webkit-mask-image: url(./assets/add.svg);\r\n        mask-image: url(./assets/add.svg);\r\n        padding-right: 8px;\r\n        /* -webkit-mask-image: url(./assets/user.svg);\r\n        mask-image: url(./assets/user.svg); */\r\n    }\r\n\r\n    .trashIcon {\r\n        width: 30px;\r\n        height: 30px;\r\n        display: inline-block;\r\n        vertical-align: sub;\r\n        color: rgba(219, 219, 219, 0.829);\r\n    }\r\n\r\n\r\n.show-hover {\r\n    display: none;\r\n}\r\n\r\n.show:hover .show-hover {\r\n    display: inline-block;\r\n}\r\n\r\n/* MODAL STYLE */\r\n\r\n  .close {\r\n    color: #aaa;\r\n    float: right;\r\n    font-size: 28px;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .close:hover,\r\n  .close:focus {\r\n    color: black;\r\n    text-decoration: none;\r\n    cursor: pointer;\r\n  }"},"lineCount":null}},"error":null,"hash":"765dafe305d56bdd50ccf6dc8f801d4c","cacheData":{"env":{}}}